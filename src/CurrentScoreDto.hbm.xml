<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.gc.dto.CurrentScoreDto" table="CurrentScore"
		schema="outingsapp">

		<id name="currentScoreID">
			<column name="currentScoreID" sql-type="int"></column>
		</id>

		<property name="totalScore">
			<column name="totalScore" sql-type="int"></column>
		</property>
		<property name="restaurantID">
			<column name="restaurantID" sql-type="int"></column>
		</property>
	
	</class>
</hibernate-mapping>