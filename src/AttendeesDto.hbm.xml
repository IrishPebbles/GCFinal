<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.gc.dto.AttendeesDto" table="Attendees"
		schema="outingsapp">


		
		<property name="outingID">
			<column name="outingID" sql-type="int"></column>
		</property>
		<property name="personID">
			<column name="personID" sql-type="int"></column>
		</property>
	</class>
</hibernate-mapping>