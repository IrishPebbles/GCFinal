<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.gc.dto.OutingDto" table="Outing"
		schema="outingsapp">


		<id name="outingID">
				<column name="outingID" sql-type="int"></column>
		</id>
		
		<property name="surveyID">
		<column name="surveyID" sql-type="varchar(255)" length="255"></column>
		</property>
				<property name="outingName">
		<column name="outName" sql-type="varchar(50)" length="50"></column>
		</property>
		<property name="dateOfEvent">
			<column name="dateOfEvent" sql-type="date"></column>
		</property>
		<property name="finalLocation">
			<column name="finalLocation" sql-type="varchar(255)" length="255"></column>
		</property>
		<property name="organizer">
			<column name="organizer" sql-type="int"></column>
		</property>
		
	</class>
</hibernate-mapping>