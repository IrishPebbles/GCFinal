<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.gc.dto.PersonDto" table="Restaurant"
		schema="outingsapp">


		<id name="restaurantID">
			
			<column name="restaurantID" sql-type="int"></column>
		</id>

		<property name="restaurantLocation">
			<column name="location" sql-type="varchar(255)" length="255"></column>
		</property>
		<property name="restaurantRating">
			<column name="rating" sql-type="double"></column>
		</property>
	</class>
</hibernate-mapping>